[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory100.png"}, {"Id": "2", "ActionId": "1000", "Type": "401", "Params[1]": "1", "Params[2]": "202"}, {"Id": "3", "ActionId": "2", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "4", "ActionId": "3", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "???", "Params[4]": "1"}, {"Id": "5", "ActionId": "4", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "6", "ActionId": "5", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "7", "ActionId": "6", "Type": "301", "Params[1]": "1", "Params[2]": "Cyril", "Params[3]": "I was going to make the introduction myself. How rude of me.", "Params[4]": "1"}, {"Id": "8", "ActionId": "7", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "9", "ActionId": "8", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "10", "ActionId": "9", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "Why...?!", "Params[4]": "1"}, {"Id": "11", "ActionId": "10", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "12", "ActionId": "11", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "13", "ActionId": "12", "Type": "301", "Params[1]": "1", "Params[2]": "Cyril", "Params[3]": "I just wanted to tell everyone the truth.", "Params[4]": "1"}, {"Id": "14", "ActionId": "13", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "15", "ActionId": "14", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "16", "ActionId": "15", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "The truth?", "Params[4]": "1"}, {"Id": "17", "ActionId": "16", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "18", "ActionId": "17", "Type": "301", "Params[2]": "System Voice", "Params[3]": "Warning. Radar waveform detected."}, {"Id": "19", "ActionId": "1001", "Type": "401", "Params[1]": "2", "Params[2]": "10007502"}, {"Id": "20", "ActionId": "18", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "21", "ActionId": "19", "Type": "301", "BeginAnim": "RImgBg1Shake", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "Wait!!!", "Params[4]": "1"}, {"Id": "22", "ActionId": "20", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "23", "ActionId": "21", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "24", "ActionId": "22", "Type": "301", "Params[1]": "1", "Params[2]": "Cyril", "Params[3]": "All the truth, pedantic, dirty, hypocritical truth... Right here!", "Params[4]": "1"}, {"Id": "25", "ActionId": "23", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "26", "ActionId": "24", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "27", "ActionId": "25", "Type": "301", "Params[2]": "System Voice", "Params[3]": "Warning. Warning. Main wings damaged. Fuel leaking!"}, {"Id": "28", "ActionId": "26", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "29", "ActionId": "27", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "What are you planning, Cyril?!", "Params[4]": "1"}, {"Id": "30", "ActionId": "28", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "(Dammit! I can't stop him if I don't attack!!)", "Params[4]": "1"}, {"Id": "31", "ActionId": "1002", "Type": "401", "Params[1]": "1", "Params[2]": "211"}, {"Id": "32", "ActionId": "29", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "33", "ActionId": "30", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "34", "ActionId": "31", "Type": "301", "Params[1]": "1", "Params[2]": "Cyril", "Params[3]": "Watanabe, do you think they can be forgiven?", "Params[4]": "1"}, {"Id": "35", "ActionId": "32", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "36", "ActionId": "33", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "37", "ActionId": "34", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "What...", "Params[4]": "1"}, {"Id": "38", "ActionId": "35", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "39", "ActionId": "36", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "40", "ActionId": "37", "Type": "301", "Params[1]": "1", "Params[2]": "Cyril", "Params[3]": "You watched my son die in front of you. Aren't you his best friend? Aren't you going to fight for him? Can you forgive these bastards?", "Params[4]": "1"}, {"Id": "41", "ActionId": "38", "Type": "301", "Params[2]": "Cyril", "Params[3]": "You saw that list! They're trying to flee into space, leaving the soldiers\u2014who are fighting with their lives\u2014behind to die! No honor, no graves!", "Params[4]": "1"}, {"Id": "42", "ActionId": "39", "Type": "301", "Params[2]": "Cyril", "Params[3]": "Can you forgive those liars?!!", "Params[4]": "1"}, {"Id": "43", "ActionId": "40", "Type": "301", "Params[2]": "Cyril", "Params[3]": "Out of my way! I will reveal their dirty deeds and show the truth to everyone!", "Params[4]": "1"}, {"Id": "44", "ActionId": "41", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "45", "ActionId": "42", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "46", "ActionId": "43", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "...", "Params[4]": "1"}, {"Id": "47", "ActionId": "44", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "I can.", "Params[4]": "1"}, {"Id": "48", "ActionId": "45", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "49", "ActionId": "46", "Type": "301", "Params[3]": "Watanabe's voice feels like fuel and lubricating oil. He shouted with unrestricted anger."}, {"Id": "50", "ActionId": "47", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "51", "ActionId": "48", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "You are not the only one who lost someone important!", "Params[4]": "1"}, {"Id": "52", "ActionId": "49", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "I have never forgotten Bruce!! And my other brothers in arms! I remember them, every single one of them!", "Params[4]": "1"}, {"Id": "53", "ActionId": "50", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "54", "ActionId": "51", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "55", "ActionId": "52", "Type": "301", "Params[2]": "Cyril", "Params[3]": "January 22nd, 2161. The northern defense was breached by Corrupted due to a mistake of the local army. Mercenary squad \"Oasis\" was hired to make up for the lack of troops.", "Params[4]": "1"}, {"Id": "56", "ActionId": "53", "Type": "301", "Params[2]": "Cyril", "Params[3]": "Oasis Squad persisted in the snow for a week and rescued all the civilians in a church. Fifty-two KIA.", "Params[4]": "1"}, {"Id": "57", "ActionId": "54", "Type": "301", "Params[2]": "Cyril", "Params[3]": "That's all the record they had in Babylonia's system.", "Params[4]": "1"}, {"Id": "58", "ActionId": "55", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "59", "ActionId": "56", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "60", "ActionId": "57", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "...", "Params[4]": "1"}, {"Id": "61", "ActionId": "58", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Albert, we call him green-head. Gaza, always had mint leaves with him. Coates, he came from Islay Islands, just like you and Bruce...", "Params[4]": "1"}, {"Id": "62", "ActionId": "59", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "No matter what... I cannot give a reasonable explanation for that list. But what I know is...", "Params[4]": "1"}, {"Id": "63", "ActionId": "60", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Those people evacuated to Babylonia... The first batch of them were researchers of anti-Punishing technology.", "Params[4]": "1"}, {"Id": "64", "ActionId": "61", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "The second batch is... It may sound like an excuse to you.", "Params[4]": "1"}, {"Id": "65", "ActionId": "62", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "But they're necessary for regulating society and spreading culture after the war ends.", "Params[4]": "1"}, {"Id": "66", "ActionId": "63", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "We live in a dark era, but we need someone to look to the future.", "Params[4]": "1"}, {"Id": "67", "ActionId": "64", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "This is what \"bird of hope\" means.", "Params[4]": "1"}, {"Id": "68", "ActionId": "65", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "69", "ActionId": "66", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "70", "ActionId": "67", "Type": "301", "Params[2]": "Cyril", "Params[3]": "...", "Params[4]": "1"}, {"Id": "71", "ActionId": "68", "Type": "301", "Params[2]": "Cyril", "Params[3]": "...What about my hope...? There are tens of thousands of soldiers like me, abandoned, stranded on the ground. What about our hope?", "Params[4]": "1"}, {"Id": "72", "ActionId": "69", "Type": "301", "Params[2]": "Cyril", "Params[3]": "Maybe you're right, young man. But I have made my decision.", "Params[4]": "1"}, {"Id": "73", "ActionId": "70", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "74", "ActionId": "71", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "75", "ActionId": "72", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "Captain Cyril...", "Params[4]": "1"}, {"Id": "76", "ActionId": "73", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Bruce used to talk about you.", "Params[4]": "1"}, {"Id": "77", "ActionId": "74", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "78", "ActionId": "75", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "79", "ActionId": "76", "Type": "301", "Params[2]": "Cyril", "Params[3]": "What... did he say?", "Params[4]": "1"}, {"Id": "80", "ActionId": "77", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "81", "ActionId": "78", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "82", "ActionId": "79", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "He said you were a great pilot. He looked up to you. He became a mercenary because he wanted to become one like you.", "Params[4]": "1"}, {"Id": "83", "ActionId": "80", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "He never hated anyone, even after the war took his family.", "Params[4]": "1"}, {"Id": "84", "ActionId": "81", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "My biggest regret... Is that I couldn't save him.", "Params[4]": "1"}, {"Id": "85", "ActionId": "82", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Bruce was unique. But his legacy will go on. I will never stop fighting on this land, even just for his sake.", "Params[4]": "1"}, {"Id": "86", "ActionId": "83", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "I will carry on his legacy.", "Params[4]": "1"}, {"Id": "87", "ActionId": "84", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "I'm sorry, but I have to stop you!", "Params[4]": "1"}, {"Id": "88", "ActionId": "85", "Type": "201", "Params[1]": "1", "Params[2]": "500106"}, {"Id": "89", "ActionId": "86", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "1"}, {"Id": "90", "ActionId": "87", "Type": "301", "Params[2]": "Cyril", "Params[3]": "Alright. Come on then, kid!! I've never lost to anyone in the air!!!", "Params[4]": "1"}, {"Id": "91", "ActionId": "88", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[4]": "1"}, {"Id": "92", "ActionId": "89", "Type": "201", "Params[1]": "1", "Params[2]": "500080"}, {"Id": "93", "ActionId": "90", "Type": "301", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "What a pity, you never got to fight Bruce!", "Params[4]": "1"}, {"Id": "94", "ActionId": "91", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Watanabe", "Params[3]": "But I'm just as good!", "Params[4]": "1"}]