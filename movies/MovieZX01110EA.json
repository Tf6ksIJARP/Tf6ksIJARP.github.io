UiStory/FxUiStoryGanRanGreen.prefab", "Params[4]": "2"}, {"Id": "17", "ActionId": "13", "Type": "302", "EndAnim": "DialogDisable", "Params[2]": "(Draw your gun)", "Params[3]": "2000", "Params[4]": "(Run to Lucia)", "Params[5]": "2001"}, {"Id": "18", "ActionId": "2000", "Type": "401", "Params[1]": "2", "Params[2]": "10001004"}, {"Id": "19", "ActionId": "2001", "Type": "502", "BeginDelay": "500", "Params[1]": "RImgBg1Shake"}, {"Id": "20", "ActionId": "2002", "Type": "501", "EndAnim": "PanelActor1Shake", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryError.prefab", "Params[2]": "1"}, {"Id": "21", "ActionId": "2005", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryGanRanNew.prefab", "Params[3]": "1"}, {"Id": "22", "ActionId": "2003", "Type": "505", "BeginDelay": "2000", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryFunction.prefab"}, {"Id": "23", "ActionId": "3070", "Type": "505", "BeginDelay": "2000", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryGanRanNew.prefab", "Params[4]": "1"}, {"Id": "24", "ActionId": "5000", "Type": "505", "BeginDelay": "2000", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryError.prefab"}, {"Id": "25", "ActionId": "2007", "Type": "301", "BeginAnim": "DialogEnable", "BeginDelay": "3000", "Params[1]": "1", "Params[2]": "Lucia", "Params[3]": "I'm okay... Commandant.", "Params[4]": "1"}, {"Id": "26", "ActionId": "2008", "Type": "302", "Params[2]": "Thank goodness.", "Params[3]": "2009", "Params[4]": "Glad to hear.", "Params[5]": "2009"}, {"Id": "27", "ActionId": "2009", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "28", "ActionId": "2010", "Type": "301", "Params[3]": "The moment the transfer fails, Qu collapses onto the ground as if all the will to fight is gone in an instant."}, {"Id": "29", "ActionId": "2011", "Type": "201", "Params[1]": "1", "Params[2]": "500150", "Params[3]": "14"}, {"Id": "30", "ActionId": "2012", "Type": "301", "Params[1]": "1", "Params[2]": "\"Qu\"", "Params[3]": "Do you really have to help them... Huaxu? Even if it means disobeying me?", "Params[4]": "1"}, {"Id": "31", "ActionId": "2013", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "You know full well... You're running out of time.", "Params[4]": "1"}, {"Id": "32", "ActionId": "2014", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "According to the experiment, the mind transfer has a higher success rate if conducted inside your M.I.N.D.", "Params[4]": "1"}, {"Id": "33", "ActionId": "2015", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "But if the transfer is stopped midway, your M.I.N.D. will collapse.", "Params[4]": "1"}, {"Id": "34", "ActionId": "2016", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "There's no time for... finding any more vessels...", "Params[4]": "1"}, {"Id": "35", "ActionId": "2017", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "36", "ActionId": "2018", "Type": "301", "Params[3]": "\"Qu\" keeps reaching into the air, as if groping for something."}, {"Id": "37", "ActionId": "2019", "Type": "201", "Params[1]": "1", "Params[2]": "500150", "Params[3]": "14"}, {"Id": "38", "ActionId": "2020", "Type": "301", "EndAnim": "AniZhuanchangBegin", "Params[1]": "1", "Params[2]": "\"Qu\"", "Params[3]": "For the last time... I command you, Huaxu.", "Params[4]": "1"}, {"Id": "39", "ActionId": "2021", "Type": "101", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory0.png"}, {"Id": "40", "ActionId": "15", "Type": "201", "Params[1]": "1", "Params[2]": "500150", "Params[4]": "-400"}, {"Id": "41", "ActionId": "1000", "Type": "201", "Params[1]": "2", "Params[2]": "500160", "Params[4]": "400"}, {"Id": "42", "ActionId": "1001", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryKejiBg.prefab", "Params[2]": "1"}, {"Id": "43", "ActionId": "1002", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryFunction.prefab", "Params[2]": "1"}, {"Id": "44", "ActionId": "1003", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryGanRanGreen.prefab", "Params[3]": "2"}, {"Id": "45", "ActionId": "16", "Type": "301", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "1", "Params[2]": "Lee", "Params[3]": "...Qu seems to be trying to let Huaxu into her M.I.N.D."}, {"Id": "46", "ActionId": "17", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "I will give you half of my world, Huaxu.", "Params[4]": "1"}, {"Id": "47", "ActionId": "1004", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "And you will be truly free.", "Params[4]": "1"}, {"Id": "48", "ActionId": "3054", "Type": "203", "Params[1]": "1", "Params[2]": "1"}, {"Id": "49", "ActionId": "3055", "Type": "203", "Params[1]": "2", "Params[2]": "1"}, {"Id": "50", "ActionId": "3056", "Type": "202", "Params[1]": "2"}, {"Id": "51", "ActionId": "1005", "Type": "301", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Huaxu", "Params[3]": "...I'm sorry.", "Params[4]": "2"}, {"Id": "52", "ActionId": "2022", "Type": "101", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory167.jpg"}, {"Id": "53", "ActionId": "1007", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryGanRanGreen.prefab", "Params[4]": "2"}, {"Id": "54", "ActionId": "2023", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryKejiBg.prefab"}, {"Id": "55", "ActionId": "2024", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryFunction.prefab"}, {"Id": "56", "ActionId": "4000", "Type": "202", "Params[1]": "1", "Params[2]": "2", "Params[6]": "1"}, {"Id": "57", "ActionId": "2025", "Type": "504", "Params[1]": "0.7"}, {"Id": "58", "ActionId": "3057", "Type": "401", "Params[1]": "1", "Params[2]": "10"}, {"Id": "59", "ActionId": "2026", "Type": "201", "Params[1]": "1", "Params[2]": "500160"}, {"Id": "60", "ActionId": "18", "Type": "301", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "1", "Params[2]": "Huaxu", "Params[3]": "What is love, Villier?"}, {"Id": "61", "ActionId": "2027", "Type": "301", "Params[2]": "Villier", "Params[3]": "It's a biological program."}, {"Id": "62", "ActionId": "2028", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "So it's something I'll never have?"}, {"Id": "63", "ActionId": "2029", "Type": "301", "Params[2]": "Villier", "Params[3]": "You will. You are better than any human."}, {"Id": "64", "ActionId": "2030", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "And what is freedom?"}, {"Id": "65", "ActionId": "2031", "Type": "301", "Params[2]": "Villier", "Params[3]": "It's a state where you're free from any control."}, {"Id": "66", "ActionId": "2032", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "You mean like you? Having a physical body and the ability to act without the input of any command?"}, {"Id": "67", "ActionId": "2033", "Type": "301", "Params[2]": "Villier", "Params[3]": "Yes, pretty much."}, {"Id": "68", "ActionId": "2034", "Type": "301", "Params[2]": "Villier", "Params[3]": "If you have a body, what's the first thing you want to do?"}, {"Id": "69", "ActionId": "2035", "Type": "301", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Huaxu", "Params[3]": "...I want a hug."}, {"Id": "70", "ActionId": "2036", "Type": "101", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory162.png"}, {"Id": "71", "ActionId": "2037", "Type": "504", "Params[1]": "0"}, {"Id": "72", "ActionId": "2038", "Type": "201", "Params[1]": "1", "Params[2]": "500150", "Params[3]": "14", "Params[6]": "1"}, {"Id": "73", "ActionId": "3058", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryGanRanNew.prefab", "Params[3]": "1"}, {"Id": "74", "ActionId": "5091", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryKejiBg.prefab", "Params[2]": "1"}, {"Id": "75", "ActionId": "5090", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryFunction.prefab", "Params[2]": "1"}, {"Id": "76", "ActionId": "2039", "Type": "301", "BeginAnim": "AniZhuanchangEnd", "EndAnim": "PanelActor1Shake", "Params[3]": "\"Qu\", as if entering into a self-embrace, retracts the extended arm. Raising the blade, \"Qu\" sends the blade within."}, {"Id": "77", "ActionId": "5099", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "78", "ActionId": "3059", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryGanRanNew.prefab", "Params[4]": "1"}, {"Id": "79", "ActionId": "5092", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryFunction.prefab"}, {"Id": "80", "ActionId": "5093", "Type": "505", "Params[2]": "Assets/Product/Effect/Prefab/FxUi/FxUiStory/FxUiStoryKejiBg.prefab"}, {"Id": "81", "ActionId": "36", "Type": "506", "Params[1]": "PanelAni", "Params[2]": "Assets/Product/Ui/ComponentPrefab/MovieAni/MovieMain11Ani01.prefab", "Params[3]": "AnimEnable"}, {"Id": "82", "ActionId": "37", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "...True freedom..."}, {"Id": "83", "ActionId": "38", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "...is to act on your own will, not on the commands of others..."}, {"Id": "84", "ActionId": "39", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "I still don't know what love is."}, {"Id": "85", "ActionId": "40", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "But because you're human, because you used to be human, I wish that... humanity will still live on, with hope in their hearts."}, {"Id": "86", "ActionId": "41", "Type": "301", "Params[2]": "Huaxu", "Params[3]": "Don't hurt anyone on my account, Villier."}, {"Id": "87", "ActionId": "42", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "...And here I am, believing that you have a mind of your own. Yet I was the one who kept ignoring your intent."}, {"Id": "88", "ActionId": "43", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "The one who deprived you of freedom, the one who kept controlling you, was me..."}, {"Id": "89", "ActionId": "44", "Type": "301", "Params[2]": "\"Qu\"", "Params[3]": "I'm glad that at least in the end... I gave you... true freedom."}, {"Id": "90", "ActionId": "45", "Type": "301", "Params[3]": "The ground shake. The buildings begin to collapse\u2014a sign that this space is rejecting the mind occupying it."}, {"Id": "91", "ActionId": "46", "Type": "301", "Params[3]": "In the midst of chaos, we catch a glimpse of a figure next to \"Qu\"."}, {"Id": "92", "ActionId": "47", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[3]": "The two stay locked in an embrace until they both fade away."}]