[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgCg/Cgqz1008.jpg"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "207"}, {"Id": "3", "ActionId": "3", "Type": "301", "BeginAnim": "DialogEnable", "Params[2]": "Electronic Voice:", "Params[3]": "Message has been sent via the nearest relay point."}, {"Id": "4", "ActionId": "4", "Type": "301", "Params[2]": "Selena", "Params[3]": "\u2026\u2026Ayla"}, {"Id": "5", "ActionId": "5", "Type": "301", "Params[2]": "Selena", "Params[3]": "...I'm sorry."}, {"Id": "6", "ActionId": "6", "Type": "301", "Params[2]": "Selena", "Params[3]": "Looks like this will be my last message."}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[2]": "Selena", "Params[3]": "But at least my \"Tempest\" has reached you, right?"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Selena", "Params[3]": "Ayla, I once told you why I wanted to become a Construct..."}, {"Id": "9", "ActionId": "9", "Type": "301", "Params[2]": "Selena", "Params[3]": "\"Our paths would be rectified if we could portray the world as it truly is.\""}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[2]": "Selena", "Params[3]": "I still believe in it."}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Selena", "Params[3]": "I'm sorry for being selfish."}, {"Id": "12", "ActionId": "12", "Type": "301", "Params[2]": "Selena", "Params[3]": "I'm sorry, Ayla. I chose my own path without letting you know..."}, {"Id": "13", "ActionId": "13", "Type": "301", "Params[2]": "Selena", "Params[3]": "Even when I know we are so similar."}, {"Id": "14", "ActionId": "14", "Type": "401", "Params[1]": "2", "Params[2]": "1506"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[3]": "Another Corrupted falls to the ground from Selena's attack."}, {"Id": "16", "ActionId": "16", "Type": "301", "Params[2]": "Selena", "Params[3]": "So I'm not alone."}, {"Id": "17", "ActionId": "17", "Type": "301", "Params[2]": "Selena", "Params[3]": "I always feel that you are next to me, fighting together."}, {"Id": "18", "ActionId": "18", "Type": "401", "Params[1]": "2", "Params[2]": "1508"}, {"Id": "19", "ActionId": "19", "Type": "301", "Params[2]": "Corrupted", "Params[3]": "\u2014\u2014\u2014\u2014! \u2014\u2014\u2014\u2014\u2014\u2014!"}, {"Id": "20", "ActionId": "20", "Type": "301", "Params[2]": "Selena", "Params[3]": "Looks like this is the end."}, {"Id": "21", "ActionId": "21", "Type": "301", "Params[2]": "Selena", "Params[3]": "My final solo..."}, {"Id": "22", "ActionId": "22", "Type": "301", "Params[2]": "Selena", "Params[3]": "Time to... call it curtains."}, {"Id": "23", "ActionId": "23", "Type": "301", "Params[2]": "Selena", "Params[3]": "Adios... (Goodbye...)"}, {"Id": "24", "ActionId": "24", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Selena", "Params[3]": "...Mi amor (...My dear friend)"}]