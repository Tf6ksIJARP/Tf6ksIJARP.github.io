[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory14.png"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "205"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "1", "Params[2]": "1081001", "Params[3]": "2"}, {"Id": "4", "ActionId": "4", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Kamui", "Params[3]": "Yo! Commandant, what are you doing here sneaking around?", "Params[4]": "1"}, {"Id": "5", "ActionId": "5", "Type": "302", "Params[2]": "Shh\u2014", "Params[3]": "6", "Params[4]": "Quiet.", "Params[5]": "6"}, {"Id": "6", "ActionId": "6", "Type": "204", "Params[1]": "1", "Params[2]": "1"}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[2]": "Kamui", "Params[3]": "Wow... I thought you were spying on something top secret but what's so interesting about Lee? Just go in and say hi... Hi! Lee!!", "Params[4]": "1"}, {"Id": "8", "ActionId": "8", "Type": "203", "Params[1]": "1", "Params[2]": "0.25", "Params[3]": "-400"}, {"Id": "9", "ActionId": "9", "Type": "201", "Params[1]": "2", "Params[2]": "1011001", "Params[4]": "400"}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[1]": "1", "Params[2]": "Kamui", "Params[3]": "You were busy with something and I saw Commandant here peeking for a while now.", "Params[4]": "1"}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Lee", "Params[3]": "Commandant, care to explain why you were loitering outside my room?", "Params[4]": "2"}, {"Id": "12", "ActionId": "12", "Type": "302", "Params[2]": "I didn't want to disturb you.", "Params[3]": "13", "Params[4]": "I was just passing by.", "Params[5]": "13"}, {"Id": "13", "ActionId": "13", "Type": "301", "Params[2]": "Lee", "Params[3]": "...Just let me know next time, I wouldn't bar you from entering.", "Params[4]": "2"}, {"Id": "14", "ActionId": "14", "Type": "301", "Params[2]": "Kamui", "Params[3]": "See, I told you to just go and say hi!", "Params[4]": "1"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[2]": "Kamui", "Params[3]": "But I'm curious about what you were doing too.", "Params[4]": "1"}, {"Id": "16", "ActionId": "16", "Type": "301", "Params[2]": "Lee", "Params[3]": "...It's none of your business.", "Params[4]": "2"}, {"Id": "17", "ActionId": "17", "Type": "301", "Params[2]": "Lee", "Params[3]": "Don't you two have a mission to do or something other than spying on me?", "Params[4]": "2"}, {"Id": "18", "ActionId": "18", "Type": "302", "Params[2]": "Checking up on your wellbeing is also my job.", "Params[3]": "19"}, {"Id": "19", "ActionId": "19", "Type": "301", "Params[2]": "Lee", "Params[3]": "...Look all you want, I'm quite sure you'll lose interest soon enough.", "Params[4]": "2"}, {"Id": "20", "ActionId": "20", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Kamui", "Params[3]": "What about me? I'm interested even if Commandant isn't!", "Params[4]": "1"}]