[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory60.jpg"}, {"Id": "2", "ActionId": "1000", "Type": "401", "Params[1]": "1", "Params[2]": "207"}, {"Id": "3", "ActionId": "2", "Type": "201", "Params[1]": "1", "Params[2]": "500024", "Params[4]": "-450"}, {"Id": "4", "ActionId": "3", "Type": "201", "Params[1]": "2", "Params[2]": "500022", "Params[4]": "450"}, {"Id": "5", "ActionId": "4", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Snow", "Params[3]": "... Bless us, O Lord, and these, Thy gifts, which we are about to receive from Thy bounty.", "Params[4]": "2"}, {"Id": "6", "ActionId": "5", "Type": "301", "Params[2]": "Snow", "Params[3]": "Through Christ, our Lord. Amen.", "Params[4]": "2"}, {"Id": "7", "ActionId": "6", "Type": "301", "Params[2]": "Bianca and Children", "Params[3]": "... Bless us, O Lord, and these, Thy gifts, which we are about to receive from Thy bounty.", "Params[4]": "1"}, {"Id": "8", "ActionId": "7", "Type": "301", "Params[2]": "Bianca and Children", "Params[3]": "Through Christ, our Lord. Amen.", "Params[4]": "1"}, {"Id": "9", "ActionId": "8", "Type": "202", "Params[1]": "2", "Params[6]": "1"}, {"Id": "10", "ActionId": "9", "Type": "203", "Params[1]": "1", "Params[2]": "0.25"}, {"Id": "11", "ActionId": "10", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Alright, everyone. We may eat.", "Params[4]": "1"}, {"Id": "12", "ActionId": "11", "Type": "301", "Params[2]": "Little Boy", "Params[3]": "Finally!"}, {"Id": "13", "ActionId": "12", "Type": "301", "Params[2]": "Little Girl", "Params[3]": "Sister Bianca, is it because of us that there's no more food?"}, {"Id": "14", "ActionId": "13", "Type": "301", "Params[2]": "Little Girl", "Params[3]": "(Whispers) There's nothing left to eat in the church..."}, {"Id": "15", "ActionId": "14", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Are you still hungry, Ada?", "Params[4]": "1"}, {