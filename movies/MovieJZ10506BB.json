[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory0.png"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "203"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "1", "Params[2]": "500040"}, {"Id": "4", "ActionId": "4", "Type": "301", "BeginAnim": "DialogEnable", "EndAnim": "AniZhuanchangBegin", "Params[1]": "1", "Params[2]": "Nanami", "Params[3]": "I found... Mother... She's with Jane... They're... okay... Thank God...", "Params[4]": "1"}, {"Id": "5", "ActionId": "5", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "6", "ActionId": "6", "Type": "101", "Params[1]": "Assets/Product/Texture/Image/BgCg/Cgqz302.jpg"}, {"Id": "7", "ActionId": "7", "Type": "301", "BeginAnim": "AniZhuanchangEnd", "Params[3]": "\u2026\u2026\u2026\u2026\u2026\u2026"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Crowd", "Params[3]": "The troops! The troops are here!"}, {"Id": "9", "ActionId": "9", "Type": "301", "Params[2]": "Crowd", "Params[3]": "In the back! The back! A group of Corrupted! There's one in the middle! Oh god, what do we do?!"}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[2]": "Guard", "Params[3]": "Everyone, stand back!"}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Guard", "Params[3]": "Take aim, ready to fire!"}, {"Id": "12", "ActionId": "12", "Type": "301", "Params[2]": "Mommy", "Params[3]": "Nanami!"}, {"Id": "13", "ActionId": "13", "Type": "301", "Params[2]": "Mommy", "Params[3]": "Don't shoot! Nanami, she's a... good child..."}, {"Id": "14", "ActionId": "14", "Type": "301", "Params[2]": "Guard", "Params[3]": "She's a mechanoid! If a mechanoid is infected\u2014"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[2]": "Mother", "Params[3]": "No! She won't! She's fighting for us... Can't you see?"}, {"Id": "16", "ActionId": "16", "Type": "301", "Params[2]": "Guard", "Params[3]": "..."}, {"Id": "17", "ActionId": "17", "Type": "301", "Params[2]": "Father", "Params[3]": "Nanami!"}, {"Id": "18", "ActionId": "18", "Type": "301", "Params[2]": "Guard", "Params[3]": "No sir! Turn back!"}, {"Id": "19", "ActionId": "19", "Type": "301", "Params[2]": "Nanami", "Params[3]": "Daddy... Mommy... Why am I... a machine...?"}, {"Id": "20", "ActionId": "20", "Type": "301", "Params[2]": "Father", "Params[3]": "Nanami... Sorry..."}, {"Id": "21", "ActionId": "21", "Type": "301", "Params[2]": "Nanami", "Params[3]": "..."}, {"Id": "22", "ActionId": "22", "Type": "301", "Params[2]": "Father", "Params[3]": "We've lied to you..."}, {"Id": "23", "ActionId": "23", "Type": "301", "Params[2]": "Father", "Params[3]": "You... were a subject in... a machine consciousness experiment..."}, {"Id": "24", "ActionId": "24", "Type": "301", "Params[2]": "Father", "Params[3]": "We treated you like a human, nurturing you so you'd grow into humans like us..."}, {"Id": "25", "ActionId": "25", "Type": "301", "Params[2]": "Nanami", "Params[3]": "But then... why can Nanami... grow..."}, {"Id": "26", "ActionId": "26", "Type": "301", "Params[2]": "Father", "Params[3]": "\"Won't grow tall if you don't sleep on time,\" remember?"}, {"Id": "27", "ActionId": "27", "Type": "301", "Params[2]": "Nanami", "Params[3]": "I see... A... Ahaha..."}, {"Id": "28", "ActionId": "28", "Type": "301", "Params[2]": "Father", "Params[3]": "Nanami... Your father and mother love you from the bottom of our hearts..."}, {"Id": "29", "ActionId": "29", "Type": "301", "Params[2]": "Father", "Params[3]": "The time we spent together was real, Nanami... We are not a fake family."}, {"Id": "30", "ActionId": "30", "Type": "301", "Params[2]": "Nanami", "Params[3]": "It's okay, I know..."}, {"Id": "31", "ActionId": "31", "Type": "301", "Params[2]": "Corrupted", "Params[3]": "Urgh\u2014"}, {"Id": "32", "ActionId": "32", "Type": "301", "Params[2]": "Crowd", "Params[3]": "Fire! Shoot them! They're coming! They're coming!"}, {"Id": "33", "ActionId": "33", "Type": "301", "Params[2]": "Guard", "Params[3]": "Evacuate the crowd! There's still one last flight in the nearby airport!"}, {"Id": "34", "ActionId": "34", "Type": "301", "Params[2]": "Nanami", "Params[3]": "Mommy, daddy, Jane, you should leave!"}, {"Id": "35", "ActionId": "35", "Type": "301", "Params[2]": "Guard", "Params[3]": "Prepare to fire!"}, {"Id": "36", "ActionId": "36", "Type": "301", "Params[2]": "Mother", "Params[3]": "Stop! You all stop!"}, {"Id": "37", "ActionId": "37", "Type": "301", "Params[2]": "Guard", "Params[3]": "We cannot take any potential risks!"}, {"Id": "38", "ActionId": "38", "Type": "301", "Params[2]": "Mother", "Params[3]": "At least... at least... just wait a little more... please..."}, {"Id": "39", "ActionId": "39", "Type": "301", "Params[2]": "Guard", "Params[3]": "...She can continue fighting, but we will not allow her to come near. And if she becomes a Corrupted, we will fire at her indiscriminately."}, {"Id": "40", "ActionId": "40", "Type": "301", "Params[2]": "Nanami", "Params[3]": "It's okay, mommy... I... I'll continue fighting the Corrupted... and protect everyone..."}, {"Id": "41", "ActionId": "41", "Type": "301", "Params[2]": "Nanami", "Params[3]": "But if I become a Corrupted... I wouldn't want myself hurting you all..."}, {"Id": "42", "ActionId": "42", "Type": "301", "Params[2]": "Nanami", "Params[3]": "Goodbye, Jane..."}, {"Id": "43", "ActionId": "43", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Nanami", "Params[3]": "Goodbye, Mommy, Daddy. So long, and thanks for all the good times..."}]