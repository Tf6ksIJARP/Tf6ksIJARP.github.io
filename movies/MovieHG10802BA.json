[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory27.jpg"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "201"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "1", "Params[2]": "1101001"}, {"Id": "4", "ActionId": "4", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Watanabe", "Params[3]": "Why are you still following me? Do you need a tour guide or something?", "Params[4]": "1"}, {"Id": "5", "ActionId": "5", "Type": "302", "Params[2]": "That would be great!", "Params[3]": "6", "Params[4]": "If that's possible.", "Params[5]": "7"}, {"Id": "6", "ActionId": "6", "NextActionId": "8", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Can't you take a hint...", "Params[4]": "1"}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "...Fine. But no funny business, I warn you.", "Params[4]": "1"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Oasis is where we reside in this wild desert.", "Params[4]": "1"}, {"Id": "9", "ActionId": "9", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "There might not be a lot of facilities here...", "Params[4]": "1"}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "But it has supported our aspiration to live.", "Params[4]": "1"}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "So, whoever dares to violate this place...", "Params[4]": "1"}, {"Id": "12", "ActionId": "12", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "...They'll have their *** handed to them, do you understand?", "Params[4]": "1"}, {"Id": "13", "ActionId": "13", "Type": "302", "Params[2]": "That's the spirit!", "Params[3]": "14", "Params[4]": "Do you need a hand?", "Params[5]": "15"}, {"Id": "14", "ActionId": "14", "NextActionId": "16", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Obviously.", "Params[4]": "1"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "Do you think I'll ask you for it?", "Params[4]": "1"}, {"Id": "16", "ActionId": "16", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "God, it makes my blood boil mentioning those invaders.", "Params[4]": "1"}, {"Id": "17", "ActionId": "17", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "I should stop talking about it and just show you.", "Params[4]": "1"}, {"Id": "18", "ActionId": "18", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "But don't let anyone know you're from up there. I won't be able to help you with what comes next.", "Params[4]": "1"}, {"Id": "19", "ActionId": "19", "Type": "302", "Params[2]": "Aren't you their leader?", "Params[3]": "20", "Params[4]": "Even when you're their leader?", "Params[5]": "20"}, {"Id": "20", "ActionId": "20", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "You're right that I'm their leader. So what do you think will happen...", "Params[4]": "1"}, {"Id": "21", "ActionId": "21", "Type": "301", "Params[2]": "Watanabe", "Params[3]": "...If they find me with a Babylonian?", "Params[4]": "1"}, {"Id": "22", "ActionId": "22", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Watanabe", "Params[3]": "You're a commandant, are you not? I'm sure you can at least understand that.", "Params[4]": "1"}]