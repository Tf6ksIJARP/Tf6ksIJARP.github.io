[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory17.jpg"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "201"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "1", "Params[2]": "1111001", "Params[4]": "-400"}, {"Id": "4", "ActionId": "4", "Type": "201", "Params[1]": "2", "Params[2]": "1091001", "Params[4]": "400"}, {"Id": "5", "ActionId": "5", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Karenina", "Params[3]": "Bianca, please?", "Params[4]": "1"}, {"Id": "6", "ActionId": "6", "Type": "301", "Params[2]": "Bianca", "Params[3]": "...", "Params[4]": "2"}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Karenina, that's impossible right now.", "Params[4]": "2"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Bianca", "Params[3]": "So, please stop asking.", "Params[4]": "2"}, {"Id": "9", "ActionId": "9", "Type": "302", "Params[2]": "What are you talking about?", "Params[3]": "10"}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[2]": "Bianca", "Params[3]": "It's nothing. I've got other matters to attend to. I'm gonna get going.", "Params[4]": "2"}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Our conversation here is over. Do as you please, Commandant.", "Params[4]": "2"}, {"Id": "12", "ActionId": "12", "Type": "202", "Params[1]": "2", "Params[6]": "1"}, {"Id": "13", "ActionId": "13", "Type": "203", "Params[1]": "1", "Params[2]": "0.25"}, {"Id": "14", "ActionId": "14", "Type": "301", "Params[2]": "Karenina", "Params[3]": "...", "Params[4]": "1"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[2]": "Karenina", "Params[3]": "I knew you were dying to ask!", "Params[4]": "1"}, {"Id": "16", "ActionId": "16", "Type": "302", "Params[2]": "Only if you want to tell me.", "Params[3]": "17", "Params[4]": "What's wrong?", "Params[5]": "18"}, {"Id": "17", "ActionId": "17", "NextActionId": "19", "Type": "301", "Params[2]": "Karenina", "Params[3]": "I don't wanna just come out and say it!", "Params[4]": "1"}, {"Id": "18", "ActionId": "18", "Type": "301", "Params[2]": "Karenina", "Params[3]": "Nothing's wrong.", "Params[4]": "1"}, {"Id": "19", "ActionId": "19", "Type": "301", "Params[2]": "Karenina", "Params[3]": "But I hate keeping secrets. I'll feel better if I just get it off my chest.", "Params[4]": "1"}, {"Id": "20", "ActionId": "20", "Type": "301", "Params[2]": "Karenina", "Params[3]": "We members of the Engineering Force work closely with the Purifying Force. We often encounter Corrupted and Constructs damaged beyond repair...", "Params[4]": "1"}, {"Id": "21", "ActionId": "21", "Type": "301", "Params[2]": "Karenina", "Params[3]": "And I just can't...", "Params[4]": "1"}, {"Id": "22", "ActionId": "22", "Type": "301", "Params[2]": "Karenina", "Params[3]": "What if the Gray Ravens become our target one day? What if I'm ordered to kill you guys?!", "Params[4]": "1"}, {"Id": "23", "ActionId": "23", "Type": "301", "Params[2]": "Karenina", "Params[3]": "I don't want my creations to be used against... Grr! Stupid emotions!", "Params[4]": "1"}, {"Id": "24", "ActionId": "24", "Type": "302", "Params[2]": "That's why you're avoiding me.", "Params[3]": "25", "Params[4]": "That won't happen to us.", "Params[5]": "26"}, {"Id": "25", "ActionId": "25", "NextActionId": "27", "Type": "301", "Params[2]": "Karenina", "Params[3]": "Yeah... That's why! So what?!", "Params[4]": "1"}, {"Id": "26", "ActionId": "26", "Type": "301", "Params[2]": "Karenina", "Params[3]": "Who's to say?!", "Params[4]": "1"}, {"Id": "27", "ActionId": "27", "Type": "301", "Params[2]": "Karenina", "Params[3]": "I made a promise to someone that I'd treasure everyone's life...", "Params[4]": "1"}, {"Id": "28", "ActionId": "28", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Karenina", "Params[3]": "I always dream that the lives of the Corrupted will be saved... Dammit...", "Params[4]": "1"}]