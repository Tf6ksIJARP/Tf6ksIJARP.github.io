[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory14.png"}, {"Id": "2", "ActionId": "990", "Type": "401", "Params[1]": "1", "Params[2]": "201"}, {"Id": "3", "ActionId": "2", "Type": "201", "Params[1]": "2", "Params[2]": "500155", "Params[4]": "-400"}, {"Id": "4", "ActionId": "3", "Type": "201", "Params[1]": "3", "Params[2]": "500149", "Params[4]": "400"}, {"Id": "5", "ActionId": "4", "Type": "501", "Params[1]": "Assets/Product/Effect/Prefab/FxUi/FxUiStoryNew/FxUiStoryTongxunHalfNew.prefab", "Params[3]": "3"}, {"Id": "6", "ActionId": "5", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "John", "Params[3]": "Strike Hawk. This is the next squad you'll be leading.", "Params[4]": "3"}, {"Id": "7", "ActionId": "6", "Type": "301", "Params[2]": "Chrome", "Params[3]": "What happened to the original squad, Mr. Smith?", "Params[4]": "2"}, {"Id": "8", "ActionId": "7", "Type": "301", "Params[2]": "John", "Params[3]": "It's useless now, and this isn't what you need to focus on. The Science Council has it all sorted out.", "Params[4]": "3"}, {"Id": "9", "ActionId": "8", "Type": "301", "Params[2]": "Chrome", "Params[3]": "...Can they never return to the battlefield again?", "Params[4]": "2"}, {"Id": "10", "ActionId": "9", "Type": "301", "Params[2]": "John", "Params[3]": "They will be useful in their own way. Such is war.", "Params[4]": "3"}, {"Id": "11", "ActionId": "10", "Type": "301", "Params[2]": "John", "Params[3]": "That's the best option. The correct answer.", "Params[4]": "3"}, {"Id": "12", "ActionId": "11", "Type": "301", "Params[2]": "John", "Params[3]": "Sacrifices have to be made.", "Params[4]": "3"}, {"Id": "13", "ActionId": "12", "Type": "301", "Params[2]": "John", "Params[3]": "Your transfer order has been approved already, thanks to how perfectly the mission went.", "Params[4]": "3"}, {"Id": "14", "ActionId": "13", "Type": "301", "Params[2]": "John", "Params[3]": "Langston... no, I should call you Chrome now. I sent in a request for you especially... You'll be the leader of this outstanding scout team that did not previously belong to any Commandant.", "Params[4]": "3"}, {"Id": "15", "ActionId": "14", "Type": "301", "Params[2]": "Chrome", "Params[3]": "But I believe my old squad is much more suited to me. And I promised the survivors that\u2014", "Params[4]": "2"}, {"Id": "16", "ActionId": "15", "Type": "301", "Params[2]": "John", "Params[3]": "Haven't I already taught you how to distinguish between the useful and the useless?", "Params[4]": "3"}, {"Id": "17", "ActionId": "16", "Type": "301", "Params[2]": "John", "Params[3]": "Tell me, Chrome. What do you want to become?", "Params[4]": "3"}, {"Id": "18", "ActionId": "17", "Type": "301", "Params[2]": "Chrome", "Params[3]": "...Smith.", "Params[4]": "2"}, {"Id": "19", "ActionId": "18", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[3]": "...Can I really become Smith?"}]