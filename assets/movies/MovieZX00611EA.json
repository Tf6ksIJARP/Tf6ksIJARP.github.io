[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory64.jpg"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "203"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "2", "Params[2]": "1021001"}, {"Id": "4", "ActionId": "4", "Type": "301", "Params[1]": "1", "Params[2]": "Lucia", "Params[3]": "Kamu...", "Params[4]": "2"}, {"Id": "5", "ActionId": "5", "Type": "202", "Params[1]": "2", "Params[6]": "1"}, {"Id": "6", "ActionId": "6", "Type": "201", "Params[1]": "1", "Params[2]": "500002"}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[1]": "1", "Params[2]": "Vera", "Params[3]": "Oh, good day, everyone!", "Params[4]": "1"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Vera", "Params[3]": "Hello, my lovely Gray Ravens.", "Params[4]": "1"}, {"Id": "9", "ActionId": "9", "Type": "301", "Params[2]": "Vera", "Params[3]": "Seems like I'm the first here again.", "Params[4]": "1"}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[3]": "Vera twirls a lock of her flowing red hair, scheming in delight."}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Vera", "Params[3]": "A Strike Hawk member corrupted... I've been longing for such a fat piece of merit.", "Params[4]": "1"}, {"Id": "12", "ActionId": "12", "Type": "202", "Params[1]": "1", "Params[6]": "1"}, {"Id": "13", "ActionId": "13", "Type": "201", "Params[1]": "2", "Params[2]": "1021001"}, {"Id": "14", "ActionId": "14", "Type": "301", "Params[1]": "1", "Params[2]": "Lucia", "Params[3]": "I'm not going to stand in your way.", "Params[4]": "2"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[2]": "Lucia", "Params[3]": "I just need to ask Kamui some questions.", "Params[4]": "2"}, {"Id": "16", "ActionId": "16", "Type": "301", "Params[2]": "Lucia", "Params[3]": "Whatever you do with him afterward, I won't have any objections.", "Params[4]": "2"}, {"Id": "17", "ActionId": "1