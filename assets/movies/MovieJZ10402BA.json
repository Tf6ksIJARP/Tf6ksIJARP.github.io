[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory60.jpg"}, {"Id": "2", "ActionId": "1001", "Type": "401", "Params[1]": "1", "Params[2]": "207"}, {"Id": "3", "ActionId": "2", "Type": "201", "Params[1]": "1", "Params[2]": "500024", "Params[4]": "-450"}, {"Id": "4", "ActionId": "3", "Type": "201", "Params[1]": "2", "Params[2]": "500022", "Params[4]": "450"}, {"Id": "5", "ActionId": "4", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Bianca", "Params[3]": "What's wrong, Father?", "Params[4]": "1"}, {"Id": "6", "ActionId": "5", "Type": "301", "Params[2]": "Snow", "Params[3]": "I heard you have been giving away your food to others, Bianca...", "Params[4]": "2"}, {"Id": "7", "ActionId": "6", "Type": "301", "Params[2]": "Snow", "Params[3]": "Here are some food, Bianca...", "Params[4]": "2"}, {"Id": "8", "ActionId": "7", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Father Snow, didn't you also give all your food to the children?", "Params[4]": "1"}, {"Id": "9", "ActionId": "8", "Type": "301", "Params[2]": "Snow", "Params[3]": "Th-that's not the same. I've already eaten.", "Params[4]": "2"}, {"Id": "10", "ActionId": "9", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Father, I have a question. What has God taught us?", "Params[4]": "1"}, {"Id": "11", "ActionId": "10", "Type": "301", "Params[2]": "Snow", "Params[3]": "...", "Params[4]": "2"}, {"Id": "12", "ActionId": "11", "Type": "301", "Params[2]": "Snow", "Params[3]": "God taught us to...", "Params[4]": "2"}, {"Id": "13", "ActionId": "12", "Type": "301", "Params[2]": "Snow", "Params[3]": "Be holy, righteous, loving, giving; to love the enemy, fast, pray; to walk in the light, to not be greedy, not to judge; to be sincere and to stay on the path.", "Params[4]": "2"}, {"Id": "14", "ActionId": "13", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Fasting.", "Params[4]": "1"}, {"Id": "15", "ActionId": "14", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Under God's and your teaching, I'm also a pious believer.", "Params[4]": "1"}, {"Id": "16", "ActionId": "15", "Type": "301", "Params[2]": "Bianca", "Params[3]": "All I have done is to obey God's teaching.", "Params[4]": "1"}, {"Id": "17", "ActionId": "16", "Type": "301", "Params[2]": "Snow", "Params[3]": "Bianca... My children.", "Params[4]": "2"}, {"Id": "18", "ActionId": "17", "Type": "301", "Params[2]": "Snow", "Params[3]": "Now, if I'm not a pious Father...", "Params[4]": "2"}, {"Id": "19", "ActionId": "18", "Type": "301", "Params[2]": "Snow", "Params[3]": "Am I qualified... as a parent...", "Params[4]": "2"}, {"Id": "20", "ActionId": "19", "Type": "301", "Params[2]": "Snow", "Params[3]": "Eat, my children, my silly children.", "Params[4]": "2"}, {"Id": "21", "ActionId": "20", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Of course you are! You raised and taught me, Father... I understand.", "Params[4]": "1"}, {"Id": "22", "ActionId": "21", "Type": "301", "Params[2]": "Snow", "Params[3]": "...", "Params[4]": "2"}, {"Id": "23", "ActionId": "22", "Type": "301", "Params[2]": "Snow", "Params[3]": "...People are starting to gather around the Snow Plain Church in the Arctic north.", "Params[4]": "2"}, {"Id": "24", "ActionId": "23", "Type": "301", "Params[2]": "Bianca", "Params[3]": "People seek shelter, and no matter their beliefs...", "Params[4]": "1"}, {"Id": "25", "ActionId": "24", "Type": "301", "Params[2]": "Bianca", "Params[3]": "As long as they need help, we will help. That is also one of your teachings.", "Params[4]": "1"}, {"Id": "26", "ActionId": "25", "Type": "301", "Params[2]": "Snow", "Params[3]": "...", "Params[4]": "2"}, {"Id": "27", "ActionId": "26", "Type": "301", "Params[2]": "Snow", "Params[3]": "How long has it been since Miyi went to town to get food? A few days?", "Params[4]": "2"}, {"Id": "28", "ActionId": "27", "Type": "301", "Params[2]": "Bianca", "Params[3]": "... It's been three days.", "Params[4]": "1"}, {"Id": "29", "ActionId": "28", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Father... Is there anything we can do?", "Params[4]": "1"}, {"Id": "30", "ActionId": "29", "Type": "301", "Params[2]": "Snow", "Params[3]": "Pray... We can only pray to God.", "Params[4]": "2"}, {"Id": "31", "ActionId": "30", "Type": "301", "Params[2]": "Snow", "Params[3]": "Let us pray... We can only pray that God will bless us in this faraway Snow Plain Church, to help the people...", "Params[4]": "2"}, {"Id": "32", "ActionId": "31", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Snow", "Params[3]": "God loves the world, Bianca.", "Params[4]": "2"}]