[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/BgStory27.jpg"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "201"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "3", "Params[2]": "1011001"}, {"Id": "4", "ActionId": "4", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Lee", "Params[3]": "There is one method.", "Params[4]": "3"}, {"Id": "5", "ActionId": "5", "Type": "301", "Params[3]": "Lee fished out a small, square-shaped pendant from his collar."}, {"Id": "6", "ActionId": "6", "Type": "301", "Params[2]": "Lee", "Params[3]": "This is a souvenir Murray gave me.", "Params[4]": "3"}, {"Id": "7", "ActionId": "7", "Type": "203", "Params[1]": "3", "Params[2]": "0.25", "Params[3]": "-400"}, {"Id": "8", "ActionId": "8", "Type": "201", "Params[1]": "2", "Params[2]": "1021001", "Params[4]": "400"}, {"Id": "9", "ActionId": "9", "Type": "301", "Params[1]": "1", "Params[2]": "Lucia", "Params[3]": "Murray... That's your little brother in Babylonia, right?", "Params[4]": "2"}, {"Id": "10", "ActionId": "10", "Type": "301", "Params[2]": "Lee", "Params[3]": "Yes, we talk occasionally.", "Params[4]": "3"}, {"Id": "11", "ActionId": "11", "Type": "301", "Params[2]": "Lucia", "Params[3]": "That's, nice...", "Params[4]": "2"}, {"Id": "12", "ActionId": "12", "Type": "301", "Params[2]": "Lee", "Params[3]": "Do you not have family in Babylonia?", "Params[4]": "3"}, {"Id": "13", "ActionId": "13", "Type": "301", "Params[2]": "Lucia", "Params[3]": "I... um...", "Params[4]": "2"}, {"Id": "14", "ActionId": "14", "Type": "204", "Params[1]": "3", "Params[2]": "10"}, {"Id": "15", "ActionId": "15", "Type": "301", "Params[2]": "Lee", "Params[3]": "Alright, it's not the time for this anyway.", "Params[4]": "3"}, {"Id": "16", "ActionId": "16", "Type": "204", "Params[1]": "3"}, {"Id": "17", "ActionId": "17", "Type": "301", "Params[2]": "Lee", "Params[3]": "This toy is a mini graviton detector. Just something you can buy in the souvenir shops near the living quarters.", "Params[4]": "3"}, {"Id": "18", "ActionId": "18", "Type": "301", "Params[2]": "Lee", "Params[3]": "\"Find me from anywhere\", that was its motto. Of course, just commercial talk.", "Params[4]": "3"}, {"Id": "19", "ActionId": "19", "Type": "301", "Params[2]": "Lee", "Params[3]": "This toy has a small detection area, is highly inaccurate, and becomes disrupted easily.", "Params[4]": "3"}, {"Id": "20", "ActionId": "20", "Type": "301", "Params[2]": "Lee", "Params[3]": "But for us, if we simply move along with it and record its reactive signals, we can theoretically deduce a vague but usable map.", "Params[4]": "3"}, {"Id": "21", "ActionId": "21", "Type": "301", "Params[2]": "Lee", "Params[3]": "And from the map, not only can we find the direction, but also the location of the projectors.", "Params[4]": "3"}, {"Id": "22", "ActionId": "22", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Lucia", "Params[3]": "Then... I'll leave it to you, Lee.", "Params[4]": "2"}]