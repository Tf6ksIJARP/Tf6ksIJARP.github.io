[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory69.jpg"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "5"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "1", "Params[2]": "500041", "Params[4]": "400"}, {"Id": "4", "ActionId": "4", "Type": "201", "Params[1]": "2", "Params[2]": "500037", "Params[4]": "-400"}, {"Id": "5", "ActionId": "5", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[2]": "Murray", "Params[3]": "You've been coming home early recently, Morian.", "Params[4]": "2"}, {"Id": "6", "ActionId": "6", "Type": "301", "Params[2]": "Morian", "Params[3]": "Your brother's doing a great job, finishing his work early. That's why he's home early to spend more time with you, Murray.", "Params[4]": "1"}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[2]": "Murray", "Params[3]": "A mechanical engineer... and working for a big company! Morian, you're amazing! And you look so cool in this outfit as well! I wonder if I'll get to be like you someday...", "Params[4]": "2"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Morian", "Params[3]": "...That day will come. You'll be even cooler and more awesome than me, kid.", "Params[4]": "1"}, {"Id": "9", "ActionId": "9", "Type": "203", "BeginAnim": "PanelActor2Shake", "Params[1]": "2", "Params[2]": "0.5", "Params[3]": "-400", "Params[4]": "-1000"}, {"Id": "10", "ActionId": "1000", "Type": "401", "Params[1]": "2", "Params[2]": "1512"}, {"Id": "11", "ActionId": "10", "Type": "301", "EndAnim": "AniZhuanchangBegin", "BeginDelay": "1000", "Params[2]": "Morian", "Params[3]": "...Murray?", "Params[4]": "1"}, {"Id": "12", "ActionId": "11", "Type": "202", "Params[1]": "1", "Params[2]": "2", "Params[6]": "1"}, {"Id": "13", "ActionId": "12", "Type": "101", "Params[1]": "Assets/Product/Texture/Image/BgCg/Cgqz402.png"}, {"Id": "14", "ActionId": "13", "Type": "301", "BeginAnim": "AniZhuanchangEnd", "Params[2]": "Morian", "Params[3]": "Dammit! It's getting worse again...?!"}, {"Id": "15", "ActionId": "14", "Type": "301", "Params[2]": "Murray", "Params[3]": "I'm... fine... Morian... Please don't be mad at me..."}, {"Id": "16", "ActionId": "15", "Type": "301", "Params[2]": "Murray", "Params[3]": "But I've been sneaking out... these past few days and... I spent all the money... I've been saving..."}, {"Id": "17", "ActionId": "16", "Type": "301", "Params[2]": "Murray", "Params[3]": "Look... brother..."}, {"Id": "18", "ActionId": "17", "Type": "301", "Params[2]": "Murray", "Params[3]": "Is this the chip... you wanted?"}, {"Id": "19", "ActionId": "18", "Type": "301", "Params[2]": "Morian", "Params[3]": "This... can't be it... Oh, Murray, you know damn well...!"}, {"Id": "20", "ActionId": "19", "Type": "301", "Params[2]": "Murray", "Params[3]": "I... didn't really know... which chip you... cough...n-needed..."}, {"Id": "21", "ActionId": "20", "Type": "301", "Params[2]": "Murray", "Params[3]": "But this one... this one... looked the best..."}, {"Id": "22", "ActionId": "21", "Type": "301", "Params[2]": "Morian", "Params[3]": "...Yes, this is the chip I needed, Murray. Exactly the one. Great job."}, {"Id": "23", "ActionId": "22", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Murray", "Params[3]": "When I can't be by your side... Then the little robot... can stand in for me... with its new chip... and stay with you, brother..."}]