[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgStory/Bgstory275.png"}, {"Id": "2", "ActionId": "2", "Type": "401", "Params[1]": "1", "Params[2]": "201"}, {"Id": "3", "ActionId": "3", "Type": "201", "Params[1]": "1", "Params[2]": "1031001", "Params[4]": "-600"}, {"Id": "4", "ActionId": "4", "Type": "201", "Params[1]": "2", "Params[2]": "500026", "Params[4]": "600"}, {"Id": "5", "ActionId": "5", "Type": "201", "Params[1]": "3", "Params[2]": "500166"}, {"Id": "6", "ActionId": "6", "Type": "301", "BeginAnim": "DialogEnable", "Params[1]": "1", "Params[3]": "A chasm breaks the ground apart, the Red Tide swirling in it. It can devour you the moment you step into it."}, {"Id": "7", "ActionId": "7", "Type": "301", "Params[2]": "Liv", "Params[3]": "The Red Tide has a high level of concentration of the Punishing Virus around it. We need to figure out a way to get past it quickly!", "Params[4]": "1"}, {"Id": "8", "ActionId": "8", "Type": "301", "Params[2]": "Lucia", "Params[3]": "I'll try to find some planks.", "Params[4]": "3"}, {"Id": "9", "ActionId": "9", "Type": "301", "EndAnim": "DialogDisable", "Params[2]": "Lee", "Params[3]": "We can try to tie some of the empty supply boxes together and walk on them.", "Params[4]": "2"}, {"Id": "10", "ActionId": "10", "Type": "302", "Params[2]": "Anyhow, we need to leave here as soon as we can.", "Params[3]": "11"}, {"Id": "11", "ActionId": "11", "Type": "305", "Params[1]": "12", "Params[2]": "21"}, {"Id": "12", "ActionId": "12", "Type": "301", "BeginAnim": "DialogEnable", "Params[2]": "Liv", "Params[3]": "What is it, Commandant?", "Params[4]": "1"}, {"Id": "13", "ActionId": "13", "Type": "302", "Params[2]": "There is a shadow in the Red Tide...", "Params[3]": "14"}, {"Id": "14", "ActionId": "14", "Type": "301", "Params[2]": "Liv", "Params[3]": "A shadow? Did you see a projection?", "Params[4]": "1"}, {"Id": "15", "ActionId": "15", "Type": "302", "Params[2]": "I think so.", "Params[3]": "16"}, {"Id": "16", "ActionId": "16", "Type": "302", "Params[2]": "And it looked like Rowler.", "Params[3]": "17"}, {"Id": "17", "ActionId": "17", "Type": "301", "Params[2]": "Liv", "Params[3]": "Is it the same person who was calling for help?", "Params[4]": "1"}, {"Id": "18", "ActionId": "18", "Type": "301", "Params[2]": "Liv", "Params[3]": "So he does say hi to people he doesn't know...", "Params[4]": "1"}, {"Id": "19", "ActionId": "19", "Type": "302", "Params[2]": "He pointed to the left. Let's go and have a look.", "Params[3]": "20"}, {"Id": "20", "ActionId": "20", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Liv", "Params[3]": "You're right. Let's go.", "Params[4]": "1"}, {"Id": "21", "ActionId": "21", "Type": "202", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[1]": "1", "Params[2]": "2", "Params[3]": "3", "Params[6]": "1"}]