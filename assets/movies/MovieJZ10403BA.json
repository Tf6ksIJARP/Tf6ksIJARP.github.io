[{"Id": "1", "ActionId": "1", "Type": "101", "BeginAnim": "AniZhuanchangEnd", "Params[1]": "Assets/Product/Texture/Image/BgCg/Cgqz703.png"}, {"Id": "2", "ActionId": "1000", "Type": "401", "Params[1]": "1", "Params[2]": "211"}, {"Id": "3", "ActionId": "2", "Type": "301", "BeginAnim": "DialogEnable", "Params[2]": "Bianca", "Params[3]": "The ultimate God, Almighty."}, {"Id": "4", "ActionId": "3", "Type": "301", "Params[2]": "Bianca", "Params[3]": "You are my faith, I follow your will and walk the path you pave."}, {"Id": "5", "ActionId": "4", "Type": "301", "Params[2]": "Bianca", "Params[3]": "Please will you listen to your pious believer's prayers..."}, {"Id": "6", "ActionId": "5", "Type": "301", "Params[2]": "Bianca", "Params[3]": "And save this church, save all your children here awaiting your help..."}, {"Id": "7", "ActionId": "6", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Wh-what's this?!"}, {"Id": "8", "ActionId": "7", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Miyi's bag! Is that... blood?!"}, {"Id": "9", "ActionId": "8", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Miyi!!"}, {"Id": "10", "ActionId": "9", "Type": "301", "Params[2]": "Child", "Params[3]": "Mama! I'm scared..."}, {"Id": "11", "ActionId": "10", "Type": "301", "Params[2]": "Mother", "Params[3]": "Don't worry, Mama's here!"}, {"Id": "12", "ActionId": "11", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Those goddamned mechanic monsters!"}, {"Id": "13", "ActionId": "12", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Watch it... You're in a church..."}, {"Id": "14", "ActionId": "13", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Ha! So?"}, {"Id": "15", "ActionId": "14", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Faith means nothing in this era!"}, {"Id": "16", "ActionId": "15", "Type": "301", "Params[2]": "Commotion", "Params[3]": "Open your eyes! Look where we are!"}, {"Id": "17", "ActionId": "16", "Type": "301", "Params[2]": "Commotion", "Params[3]": "We're stuck in here! Yet anybody is going to save us?"}, {"Id": "18", "ActionId": "17", "Type": "301", "Params[2]": "Commotion", "Params[3]": "If there wasn't this church... we were all dead!"}, {"Id": "19", "ActionId": "18", "Type": "301", "Params[2]": "Commotion", "Params[3]": "With the blizzard outside, and the mad mechanical monsters everywhere..."}, {"Id": "20", "ActionId": "19", "Type": "301", "Params[2]": "Commotion", "Params[3]": "How can we get out of here!"}, {"Id": "21", "ActionId": "20", "Type": "301", "Params[2]": "Commotion", "Params[3]": "..."}, {"Id": "22", "ActionId": "21", "Type": "301", "Params[2]": "Bianca", "Params[3]": "...I know I am a sinner who needs your forgiveness."}, {"Id": "23", "ActionId": "22", "Type": "301", "Params[2]": "Bianca", "Params[3]": "But I, Bianca, will be willing to give my life for this."}, {"Id": "24", "ActionId": "23", "Type": "301", "IsEnd": "1", "EndAnim": "AniZhuanchangBegin", "Params[2]": "Bianca", "Params[3]": "Please, save us all..."}]